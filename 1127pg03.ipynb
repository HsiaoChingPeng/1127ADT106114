    "import cv2\n",
    "\n",
    "cap = cv2.VideoCapture(0)\n",
    "\n",
    "while(True):\n",
    "    ret,frame = cap.read()\n",
    "    cv2.imshow('frame',frame)\n",
    "    if cv2.waitKey(1) ==ord('q'):\n",
    "        cv2.imwrite(\"ADT106114.png\",frame)\n",
    "        break\n",
    "        \n",
    "cap.release()\n",
    "cv2.detroyAllWindows()"
